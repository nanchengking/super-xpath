{
  "name": "super xpath",
  "manifest_version":2,
  "version": "1.0.0",
  "description": "ger xpath from selected element",
  "icons": { "128": "images/icon.png" },
  "background": {
    "persistent": false,
    "scripts": ["js/bg.js"]
  },
  "permissions": [
    "activeTab",
    "http://*.google.com/",
    "https://*.google.com/"
  ],
  "browser_action": {
    "default_title": "super xpath",
    "default_icon": "images/icon.png",
    "default_popup": "html/popup.html"
  }
}